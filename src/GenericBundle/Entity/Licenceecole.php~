<?php

namespace GenericBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * Licenceecole
 *
 * @ORM\Table(name="licenceecole", indexes={@ORM\Index(name="fk_Licence_Ecole1_idx", columns={"Ecole_idEcole"})})
 * @ORM\Entity
 */
class Licenceecole
{
    /**
     * @var integer
     *
     * @ORM\Column(name="idLicence", type="integer", nullable=false)
     * @ORM\Id
     * @ORM\GeneratedValue(strategy="IDENTITY")
     */
    private $idlicence;

    /**
     * @var string
     *
     * @ORM\Column(name="Libelle", type="string", length=45, nullable=true)
     */
    private $libelle;

    /**
     * @var string
     *
     * @ORM\Column(name="description", type="string", length=255, nullable=false)
     */
    private $description;

    /**
     * @var string
     *
     * @ORM\Column(name="DateDebut", type="string", length=45, nullable=true)
     */
    private $datedebut;

    /**
     * @var string
     *
     * @ORM\Column(name="DateFin", type="string", length=45, nullable=true)
     */
    private $datefin;

    /**
     * @var \Ecole
     *
     * @ORM\ManyToOne(targetEntity="Ecole")
     * @ORM\JoinColumns({
     *   @ORM\JoinColumn(name="Ecole_idEcole", referencedColumnName="id", onDelete="CASCADE")
     * })
     */
    private $ecoleecole;



    /**
     * Get idlicence
     *
     * @return integer
     */
    public function getIdlicence()
    {
        return $this->idlicence;
    }

    /**
     * Set libelle
     *
     * @param string $libelle
     *
     * @return Licenceecole
     */
    public function setLibelle($libelle)
    {
        $this->libelle = $libelle;

        return $this;
    }

    /**
     * Get libelle
     *
     * @return string
     */
    public function getLibelle()
    {
        return $this->libelle;
    }

    /**
     * Set datedebut
     *
     * @param string $datedebut
     *
     * @return Licenceecole
     */
    public function setDatedebut($datedebut)
    {
        $this->datedebut = $datedebut;

        return $this;
    }

    /**
     * Get datedebut
     *
     * @return string
     */
    public function getDatedebut()
    {
        return $this->datedebut;
    }

    /**
     * Set datefin
     *
     * @param string $datefin
     *
     * @return Licenceecole
     */
    public function setDatefin($datefin)
    {
        $this->datefin = $datefin;

        return $this;
    }

    /**
     * Get datefin
     *
     * @return string
     */
    public function getDatefin()
    {
        return $this->datefin;
    }

    /**
     * Set ecoleecole
     *
     * @param \GenericBundle\Entity\Ecole $ecoleecole
     *
     * @return Licenceecole
     */
    public function setEcoleecole(\GenericBundle\Entity\Ecole $ecoleecole = null)
    {
        $this->ecoleecole = $ecoleecole;

        return $this;
    }

    /**
     * Get ecoleecole
     *
     * @return \GenericBundle\Entity\Ecole
     */
    public function getEcoleecole()
    {
        return $this->ecoleecole;
    }
}
