<?php
// src/AppBundle/Entity/Users.php

namespace GenericBundle\Entity;

use FOS\UserBundle\Model\User as BaseUser;
use Doctrine\ORM\Mapping as ORM;

/**
 * @ORM\Entity
 * @ORM\Table(name="users")
 */
class Users extends BaseUser
{
    /**
     * @ORM\Id
     * @ORM\Column(type="integer")
     * @ORM\GeneratedValue(strategy="AUTO")
     */
    protected $id;

    /**
     * @var \Societe
     *
     * @ORM\ManyToOne(targetEntity="Societe")
     * @ORM\JoinColumns({
     *   @ORM\JoinColumn(name="Societe_id", referencedColumnName="id")
     * })
     */
    private $societe;

    /**
     * @var \Ecole
     *
     * @ORM\ManyToOne(targetEntity="Ecole")
     * @ORM\JoinColumns({
     *   @ORM\JoinColumn(name="ecole_id", referencedColumnName="id")
     * })
     */
    private $ecole;

    public function __construct()
    {
        parent::__construct();
        // your own logic
    }

    /**
     * Set societe
     *
     * @param \GenericBundle\Entity\Societe $societe
     *
     * @return Users
     */
    public function setSociete(\GenericBundle\Entity\Societe $societe = null)
    {
        $this->societe = $societe;

        return $this;
    }

    /**
     * Get societe
     *
     * @return \GenericBundle\Entity\Societe
     */
    public function getSociete()
    {
        return $this->societe;
    }

    /**
     * Set ecole
     *
     * @param \GenericBundle\Entity\Ecole $ecole
     *
     * @return Users
     */
    public function setEcole(\GenericBundle\Entity\Ecole $ecole = null)
    {
        $this->ecole = $ecole;

        return $this;
    }

    /**
     * Get ecole
     *
     * @return \GenericBundle\Entity\Ecole
     */
    public function getEcole()
    {
        return $this->ecole;
    }
}
